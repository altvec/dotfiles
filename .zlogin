#PATH=./bin:$PATH
#PATH=/usr/local/bin:/usr/local/sbin:$PATH
#PATH=$PATH:/sbin

# Go lang
export PATH=$PATH:/usr/local/opt/go/libexec/bin
export GOPATH=$HOME/Projects/Go

export LANG="en_US.UTF-8"
export LC_ALL="en_US.UTF-8"
