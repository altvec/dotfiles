alias zconf="subl ~/.zshrc"
alias zalia="subl ~/.zshaliases"
alias findn='find . -name '
alias df='df -h'
alias cdd='cd -'
alias psaux='ps aux | grep -v grep | grep -i'

# Python and venv
alias mkvenv2="mkvirtualenv --python /usr/local/bin/python"
alias mkvenv3="mkvirtualenv --python /usr/local/bin/python3"

# Git stuff
alias gits="clear; git status"
alias gitl="git log"
alias gitpo="git push origin"
alias gitg="git log --oneline --graph"
alias gituall="git submodule foreach git pull origin master"
